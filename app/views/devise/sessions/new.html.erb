<div class="wrapper fadeInDown">
<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div id="formContent">
      <div class="fadeIn first">
      <h1>&nbsp;</h1>
      </div>
  <%= f.email_field :email, autofocus: true, autocomplete: "email", :class => "fadeIn second login-txt", id: 'login', placeholder: 'login' %>
  <%= f.password_field :password, autocomplete: "current-password", :class => "fadeIn third login-txt", id: 'password', placeholder: 'password' %>

  <% if devise_mapping.rememberable? %>
      <div class="form-group">
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me %>
      </div>
  <% end %>
  <%= f.submit "Log in", :class => "fadeIn fourth" %>
  </div>
<% end %>
<%= render "devise/shared/links" %>
</div>